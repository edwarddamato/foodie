{
  "globals": {
    "__TS_CONFIG__": "client/tsconfig.json"
  },
  "transform": {
    ".(ts|tsx)": "../node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "moduleNameMapper": {
    "\\.(css|less|scss)$": "identity-obj-proxy"
  }
}